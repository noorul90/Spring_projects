<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="basicSecurityPkg" class="com.cm.beans.InternetSecurity">
    <property name="features">
      <list value-type="java.lang.String">
        <value>malware protection</value>
        <value>firewall</value>
      </list>
    </property>
  </bean>
  
  <bean id="advanceSecurityPkg" class="com.cm.beans.InternetSecurity" parent="basicSecurityPkg">
     <property name="features">
       <list value-type="java.lang.String" merge="true">
          <value>online shoping security</value>
          <value>spyware protection</value>
       </list>
     </property>
  </bean>
  
  <bean id="premeumSecurityPkg" class="com.cm.beans.InternetSecurity" parent="advanceSecurityPkg">
     <property name="features">
       <list value-type="java.lang.String" merge="true">
          <value>secure web browsing</value>
          <value>secure logging</value>
          <value>autofix</value>
       </list>
     </property>
  </bean>

</beans>
