<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   <bean id="car" class="com.baw.beans.Car" autowire="byType" />
   
   <bean id="engine1" class="com.baw.beans.Engine" autowire-candidate="false">
      <property name="engineNo" value="25645" />
      <property name="engineType" value="Diesel" />
   </bean>
   
   <bean id="engine2" class="com.baw.beans.Engine">
      <property name="engineNo" value="45356" />
      <property name="engineType" value="Petrol" />
   </bean>
   
</beans>
