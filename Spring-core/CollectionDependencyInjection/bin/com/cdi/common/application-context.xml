<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="btechcse1stsem" class="com.cdi.beans.Course">
      <constructor-arg>
         <list value-type="java.lang.String">
            <value>JAVA</value>
            <value>.NET</value>
            <value>PHP</value>
            <value>C++</value>
            <value>C</value>
         </list>
      </constructor-arg>
      
      <property name="faculties" >
         <set value-type="java.lang.String">
            <value>Noorul</value>
            <value>Raaj</value>
            <value>Shahid</value>
            <value>Mark</value>
            <value>George</value>
         </set>  
      </property>
      
      <property name="facultySubjects">
       <props>
         <prop key="Shahid">C</prop>
         <prop key="George">.Net</prop>
         <prop key="Mark">php</prop>
         <prop key="Noorul">java</prop>
         <prop key="Raaj">c++</prop>
       </props>
     </property> 
  </bean>
  
  
  <bean id="btechece1stsem" class="com.cdi.beans.Course">
      <constructor-arg>
         <list value-type="java.lang.String">
            <value>DCS</value>
            <value>ELS</value>
            <value>C</value>
         </list>
      </constructor-arg>
      
      <property name="faculties" >
         <set value-type="java.lang.String">
            <value>John</value>
            <value>David</value>
            <value>Ankit</value>
         </set>  
      </property>
      
      <property name="facultySubjects">
       <props>
         <prop key="John">DCS</prop>
         <prop key="David">ELS</prop>
         <prop key="Ankit">C</prop>
       </props>
     </property> 
  </bean>
   
   
  <bean id="university" class="com.cdi.beans.University"> 
     <property name="hodByDept">
       <map key-type="java.lang.String" value-type="com.cdi.beans.Course">
          <entry key="HODofCSE">
            <ref bean="btechcse1stsem" />
          </entry>
       
          <entry key="HODofMCA">
            <ref bean="btechece1stsem" />
          </entry>
       </map>
     </property>  
  </bean>
   
</beans>
