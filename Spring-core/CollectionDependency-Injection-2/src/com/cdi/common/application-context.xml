<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

   <bean id="btechcse1stsem" class="com.cdi.beans.Course">
      <constructor-arg>
         <list value-type="java.lang.String">
            <value>JAVA</value>
            <value>.NET</value>
            <value>PHP</value>
            <value>C++</value>
            <value>C</value>
         </list>
      </constructor-arg>
      
      <property name="faculties" >
         <set value-type="java.lang.String">
            <value>Noorul</value>
            <value>Raaj</value>
            <value>Shahid</value>
         </set>  
      </property>
   </bean>
   
   <bean id="btech1stsemit" class="com.cdi.beans.University">
     <property name="topperClassMap">
       <map key-type="java.lang.String" value-typ="com.cdi.beans.Course">
          <entry key="mark">
            <ref bean="btechcse1stsem">
          </entry>
          <entry key="name">
            <ref bean="btech1stsemit">
          </entry>
       </map>
     </property>
   </bean>
   
</beans>
