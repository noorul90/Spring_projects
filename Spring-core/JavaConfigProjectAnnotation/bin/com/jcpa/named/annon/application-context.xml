<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="car" class="com.jcpa.named.annon.Car" />
	
	<bean id="engine1" class="com.jcpa.named.annon.Engine" >
	  <property name="id" value="121" />
	  <property name="type" value="Petrol" />
<!-- 	  <qualifier value="myeng"/> -->
	</bean>
	
	<bean id="engine2" class="com.jcpa.named.annon.Engine" >
	  <property name="id" value="2211" />
	  <property name="type" value="Diesel" />
	</bean>
	
	<context:component-scan base-package="com.jcpa.named.*" />
	

</beans>
