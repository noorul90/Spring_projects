<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="yamahaEngine" class="com.lmi.beans.YamahaEngineImpl" scope="prototype" />
	
	<bean id="bajajEngine" class="com.lmi.beans.BajajEngineImpl" scope="prototype"/>
	
	<bean id="car" class="com.lmi.beans.Car">
	  <lookup-method name="getEngine" bean="bajajEngine" />
	</bean>

</beans>
