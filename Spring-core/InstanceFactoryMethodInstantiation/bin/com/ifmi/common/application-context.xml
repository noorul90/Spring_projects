<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

  <bean id="googleMapRendere" class="com.ifmi.beans.GoogleMapRenderer" >
    <property name="googleMapEngine" ref="usaGoogleMapEngine" />
  </bean>
  
  <bean id="googleEngineServiceLocator" class="com.ifmi.util.GoogleEngineServiceLocator" />
  
  <bean id="indiaGoogleMapEngine" factory-bean="googleEngineServiceLocator" factory-method="getIndiaGoogleEngine" />
  
  <bean id="usaGoogleMapEngine" factory-bean="googleEngineServiceLocator" factory-method="getUsaGoogleEngine" />

</beans>
